    "cli-progress": "^3.12.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.3.1",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "lucide-react": "^0.344.0",
    "next": "^14.2.5",
    "node-fetch": "^2.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^2.2.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
